{
  "name": "kpop-card-discord",
  "version": "1.0.0",
  "description": "The KpopCard discord bot",
  "main": "index.js",
  "scripts": {
    "build": "npm run-script clean && npx tsc -p . && npm run-script copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.ttf src/**/*.ejs src/**/*.html src/**/*.min.js src/**/*.css dist/",
    "run-built": "node ./dist/index.js",
    "start": "npx ts-node ./src/index.ts",
    "drop-test": "npx ts-node ./src/commonCommandLogic/__test__/uniformDrops.ts",
    "migrate": "node-pg-migrate",
    "tailwind": "npx tailwindcss --config ./src/workers/WebApp/tailwind.config.js -i ./src/workers/WebApp/css/main.css -o ./src/workers/WebApp/public/assets/output.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-format": "^0.2.4",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express-ws": "^3.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.capitalize": "^4.2.9",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/pg-escape": "^0.2.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "chokidar": "^3.5.3",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "discord-api-types": "^0.37.11",
    "discord.js": "^14.5.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
