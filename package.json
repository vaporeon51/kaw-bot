    "obscenity": "^0.1.4",
    "pg": "^8.11.3",
    "pg-escape": "^0.2.0",
    "pure-rand": "^6.0.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "unb-api": "^1.3.2",
    "undici": "^6.2.1",
    "uuid": "^9.0.1"
  }
